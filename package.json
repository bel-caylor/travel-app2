{
  "name": "Travel-App",
  "jest": {
    "verbose": true,
    "moduleFileExtensions": ["js", "jsx"],
    "moduleDirectories": ["node_modules", "bower_components", "shared"],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|svg|ttf|mp4|webm|wav|mp3|m4a)$": "<rootDir>/__test__/mocks/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/src/client/"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/src/client/styles/"
    ]
  },
  "version": "1.0.0",
  "description": "Blank App Setup",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start": "node src/server/index.js",
    "build-prod": "rm -rf dist && webpack --config webpack.prod.js",
    "build-dev": "rm -rf dist && webpack --config webpack.dev.js",
    "devs": "webpack-dev-server  --config webpack.dev.js"
  },
  "author": "Belinda",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.8.4",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "node-fetch": "^2.6.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "webpack-dev-server": "^3.10.1"
  }
}
